{
  "name": "@kwooshung/react-component-template",
  "version": "1.2.0",
  "title": "React Component Template",
  "description": "React Component Template",
  "private": true,
  "tags": [
    "kwooshung",
    "react",
    "github-page",
    "github",
    "pages",
    "page",
    "react-component"
  ],
  "keywords": [
    "kwooshung",
    "react",
    "github-page",
    "github",
    "pages",
    "page",
    "react-component"
  ],
  "author": "kwooshung (https://github.com/kwooshung)",
  "homepage": "https://github.com/kwooshung/react-component-template",
  "repository": {
    "type": "git",
    "url": "https://github.com/kwooshung/react-component-template.git"
  },
  "bugs": {
    "url": "https://github.com/kwooshung/react-component-template/issues"
  },
  "main": "dist/react-no-ssr.cjs.js",
  "module": "dist/react-no-ssr.esm.js",
  "unpkg": "dist/react-no-ssr.umd.js",
  "types": "dist/react-no-ssr.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "tsc && vite build",
    "stylelint": "stylelint --fix --cache --quiet --max-warnings 0 --config .stylelintrc.cjs \"**/*.{css,less,scss,sass,stylus,styl}\"",
    "eslint": "eslint . --fix --cache --quiet --report-unused-disable-directives --max-warnings 0 --ext .js,.cjs,.jsx,.ts,.cts,.tsx",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:ci": "vitest run --coverage",
    "commit": "pnpm changelog && git add . && cz-customizable",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -r 0 && node plugin-customizable.cjs",
    "release": "standard-version-helper"
  },
  "config": {
    "cz-customizable": {
      "config": "./.cz-config.cjs"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@arco-design/web-react": "^2.55.0",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": ">=16.6.0",
    "react-dom": ">=16.6.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^18.1.0",
    "@kwooshung/standard-version-helper": "^1.5.2",
    "@swc/core": "^1.3.95",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^29.5.6",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-v8": "^0.34.6",
    "commitizen": "^4.3.0",
    "commitlint": "^18.2.0",
    "commitlint-config-cz": "^0.13.3",
    "commitlint-config-git-commit-emoji": "^1.0.0",
    "conventional-changelog": "^5.1.0",
    "conventional-changelog-cli": "^4.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^22.1.0",
    "less": "^4.2.0",
    "lint-staged": "^15.0.2",
    "postcss-less": "^6.0.0",
    "prettier": "^3.0.3",
    "standard-version": "^9.5.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "terser": "^5.24.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.3",
    "vite-plugin-imp": "^2.4.0",
    "vitest": "^0.34.6"
  },
  "license": "MIT"
}
